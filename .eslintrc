{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "comma-dangle": 0,
    "global-require": 0,
    "no-param-reassign": 0,
    "new-cap": 0,
    "no-eval": 0
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack_configs/devServer.config.js"
      }
    }
  }
}


